{
	"name": "Syme",
	"version": "0.1.2",
	"manifest_version": 2,
	"description": "Syme: a social network for groups that encrypts everything you share.",
	"browser_action": {
		"default_icon": "/img/favicon.ico",
		"popup": "main.html"
	},
	"icons": {
		"48": "/img/logo-48x48.png",
		"128": "/img/logo-128x128.png"
	},
	"web_accessible_resources": [
    "/img/logo-48x48.png"
  ],
	"permissions": [
		"https://getsyme.com/",
		"notifications",
		"storage"
	],
	"background": {
	  "scripts": ["open.js"]
	}
}