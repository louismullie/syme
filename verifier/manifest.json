{
  "manifest_version": 2,

  "name": "Syme Verifier",
  "description": "This extension verifies the integrity of Syme's Javascript code.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "https://crypto.cat/"
  ],
  
  "content_scripts": [ {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["popup.js"],
      "run_at": "document_start"
  }]
  
}